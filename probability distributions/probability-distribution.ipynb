    "def simulate_dice_rolls(num_dice, num_rolls):\n",
    "    # Simulate rolls\n",
    "    rolls = np.random.randint(1, 7, size=(num_rolls, num_dice))\n",
    "    # Sum the dice\n",
    "    sums = np.sum(rolls, axis=1)\n",
    "    return sums"
